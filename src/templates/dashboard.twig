{# This is the dashboard template, extend it from the base template #}
{% extends "base.twig" %}

{% block title %}{{title}}{% endblock %}

{% block styles %}
    {{ parent() }}
    <link rel="stylesheet" href="dashboard.css">
{% endblock %}

{% block content %}
    <h1>Dashboard</h1>
    <p>Welcome to your dashboard. Here you can manage your settings and view your data.</p>
{% endblock %}

{% block scripts %}
    {{ parent() }}
    <script src="dashboard.js"></script>
{% endblock %}